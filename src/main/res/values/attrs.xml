    <declare-styleable name="KgNumberLayout">
        <attr name="scaleTextSize" format="dimension" />
        <attr name="kgTextSize" format="dimension" />
        <attr name="scaleTextColor" format="color" />
        <attr name="kgTextColor" format="color" />
